{
  "name": "pino-pretty",
  "version": "2.5.0",
  "description": "Prettifier for Pino log lines",
  "main": "index.js",
  "bin": {
    "pino-pretty": "./bin.js"
  },
  "scripts": {
    "lint": "standard | snazzy",
    "lint-ci": "standard",
    "pretest": "echo {} > ./.pino-prettyrc.json",
    "test": "tap --no-cov 'test/**/*.test.js'",
    "pretest-ci": "echo {} > ./.pino-prettyrc.json",
    "test-ci": "tap --cov 'test/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pinojs/pino-pretty.git"
  },
  "keywords": [
    "pino"
  ],
  "author": "James Sumners <james.sumners@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinojs/pino-pretty/issues"
  },
  "homepage": "https://github.com/pinojs/pino-pretty#readme",
  "precommit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "args": "^5.0.0",
    "chalk": "^2.3.2",
    "dateformat": "^3.0.3",
    "fast-json-parse": "^1.0.3",
    "fast-safe-stringify": "^2.0.6",
    "jmespath": "^0.15.0",
    "pump": "^3.0.0",
    "readable-stream": "^3.0.6",
    "rimraf": "^2.6.3",
    "split2": "^3.0.0"
  },
  "devDependencies": {
    "pino": "^5.9.0",
    "pre-commit": "^1.2.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "tap": "^12.1.0"
  }
}
